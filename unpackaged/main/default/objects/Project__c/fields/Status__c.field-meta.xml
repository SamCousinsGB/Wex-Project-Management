<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status__c</fullName>
    <description>Project status, based on the percentage complete.</description>
    <externalId>false</externalId>
    <formula>IF  (Percentage_Complete__c == 0, &quot;Not Started&quot;, 
    IF(Percentage_Complete__c == 1, &quot;Complete&quot;, 
        &quot;In Progress&quot;))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Project status, based on the percentage complete.</inlineHelpText>
    <label>Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
