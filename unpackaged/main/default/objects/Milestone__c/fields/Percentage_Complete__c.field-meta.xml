<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Percentage_Complete__c</fullName>
    <description>Total Completion Percentage based on related issue records.</description>
    <externalId>false</externalId>
    <formula>IF(
 Total_Number_of_Issues__c  &gt; 0,
(Total_Number_of_Completed_Issues__c / Total_Number_of_Issues__c),
0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Total Completion Percentage based on related issue records.</inlineHelpText>
    <label>% Complete</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
